<p id="notice"><%= notice %></p>

<h1>Invoices</h1>

<div class="table-responsive">
<table class="table">
  <thead>
    <tr>
      <th>Daily bill</th>
      <th>Total bill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @invoices.each do |invoice| %>
      <tr>
        <td><%= invoice.daily_bill %></td>
        <td><%= invoice.total_bill %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<br>
